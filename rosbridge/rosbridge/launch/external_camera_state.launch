<launch>
  <node name="external_camera_state" pkg="rosbridge" type="external_camera_state.py" output="screen">
    <rosparam command="delete" param=""/>
    <rosparam command="load" file="$(find rosbridge)/config/mqtt.yaml"/>
    <rosparam command="load" file="$(find rosbridge)/config/external_camera_state.yaml"/>
  </node>
</launch>
