<launch>
  <node name="robot_request" pkg="rosbridge" type="robot_request.py" output="screen">
    <rosparam command="delete" param=""/>
    <rosparam command="load" file="$(find rosbridge)/config/mqtt.yaml"/>
    <rosparam command="load" file="$(find rosbridge)/config/robot_request.yaml"/>
  </node>
</launch>
